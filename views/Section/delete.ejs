<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">

      <h2>Confirm deletion of Section</h2>
      <br>
      <br> ID: <%=Section._id %>
      <br> SectionNumber : <%=Section.SectionNumber %>
      <br> Days: <%=Section.Days %>
      <br> StartTime: <%=Section.StartTime %>
      <br> RoomNumber: <%=Section.RoomNumber %>
      <br> CourseID: <%=Section.CourseID %>
      <hr>
      
      <form method="get" action="/Section">
        <input type="submit" value="Return to list" class="btn btn-caution" />
      </form>


      <form method="POST" action="/Section/delete/<%= Section._id %>">
        <input type="submit" value="Delete this item" class="btn btn-danger pull-right" />
      </form>

    </div>
  </div>
</div>